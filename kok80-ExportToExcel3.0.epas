                 gfEnableControls, gfEnableScrollEvents, gfMoveTo, gfEOF, gfRecNo);

type TGridFunc2 =(gfColumnCount, gfFieldAsString, gfColumnCaption, gfColumnVisible, gfColumnWidth);

